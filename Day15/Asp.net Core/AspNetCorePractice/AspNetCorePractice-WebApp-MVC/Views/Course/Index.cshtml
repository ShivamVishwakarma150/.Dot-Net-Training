@model IEnumerable< AspNetCorePractice_WebApp_MVC.Models.Course>

<h1>Course List</h1>

<table class="table table-bordered w-50">

    <tr>
        <th>CourseId</th>
        <th>Course Name</th>
        <th>Technology</th>
        <th>Duration</th>
        <th>Price Of Course</th>
    </tr>
    @foreach (Course item in Model)
    {
            <tr>
                <td>@item.CourseId</td>
                <td>@item.CourseName</td>
                <td>@item.Technology</td>
                <td>@item.Duration</td>
                <td>@item.PriceOfCourse</td>
            </tr>
    }
</table>