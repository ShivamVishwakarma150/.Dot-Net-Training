@model IEnumerable< AspNetCorePractice_WebApp_MVC.Models.Trainer >

<h1>Trainers List</h1>

<table class="table table-bordered w-50">

    <tr>
        <th>Trainer Id</th>
        <th>Trainer Name</th>
        <th>Skills</th>
        <th>Years Of Experience</th>
        <th>Contact</th>
        <th>Email</th>
    </tr>
    @foreach(Trainer item in Model)
    {
        <tr>
            <td>@item.TrainerId</td>
            <td>@item.TrainerName</td>
            <td>@item.Skills</td>
            <td>@item.YearOfExperience</td>
            <td>@item.Contact</td>
            <td>@item.Email</td>
        </tr>
    }
</table>